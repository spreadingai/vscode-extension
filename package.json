{
	"name": "docuo",
	"displayName": "Docuo",
	"description": "Building documents with MDX is more powerful and easier.",
	"version": "0.0.1",
	"publisher": "spreading-docuo",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/spreadingai/vscode-extension"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": ["Programming Languages", "Snippets", "Formatters", "Notebooks"],
	"keywords": ["MDX", "markdown", "ai", "keybindings", "documentation"],
	"pricing": "Free",
	"icon": "logo.png",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"typescript": "^5.3.2"
	}
}